/*****************************  报错  **********************************/
.m_submitErrors_Main{display: none;position: fixed;top: 0;right: 0;bottom: 0;left: 0;z-index: 1112;background-color: rgba(0,0,0,0.3);}
.m_submitErrors {position: absolute;top: 50%;left: 50%;width: 600px;height: 400px;margin-top: -200px;margin-left: -300px;border: 1px solid #ccc;border-radius: 10px;background: #fff;}
.m_submitErrors .fr.c_closeimg0{top: 10px;right: 20px;}
.m_submitErrors > p{width: 100%;height: 60px;line-height: 60px;color: #64b211;font-size: 24px;text-align: center;}
.m_submitErrors .m_errorPoints{border: 1px solid #ccc;width: 140px;height: 40px;line-height: 40px;text-align: center;margin-bottom: 20px;margin-left: 30px;border-radius: 10px;color: #666;font-size: 18px;}
.m_submitErrors .m_errorPoints:hover{border: 1px solid #f9a24a;background: #f9a24a;color: #fff;}
#errorReason{border: none;outline: none;border: 1px solid #ccc;width: 540px;height: auto;min-height: 120px;line-height: 32px;margin-left: 30px;border-radius: 10px;color: #999;font-size: 18px;  }
.m_submitErrors > input{border: none;outline: none;border: 1px solid #999;background: #999;border-radius: 10px;width: 200px;height: 40px;color: #fff;margin: 20px 0 0 66px;cursor: pointer;}
.m_submitErrors > input:hover{background: #65b113;border: 1px solid #65b113;}
.m_errorPoints.e_active{background: #f9a24a;color: #fff;border: 1px solid #f9a24a;}
.t_active{color: #65b113;}

/*****************************  打印  **********************************/
.p_LessMoney{display: none; z-index: 99999; position: fixed;top: 0;left: 0;width: 100%;height: 100%;background: black;background: rgba(0,0,0,0.5);}
.p_NoIdentifyBg{width: 641px;height: 315px;position: absolute;bottom: 0;top: 0;left: 0;right: 0;margin: auto;background: #fff;border-radius: 10px;overflow: hidden;}
.p_btn{position:relative;width:218px;height:55px;top:129px;text-align: center;font-size: 18px;line-height: 55px;border-radius: 10px;cursor:pointer;float:left;background: #65b113;color: #fff;}
.p_btn:hover{background: #5da313;}
.normal{right:125px;}
.withAnalyze{left:331px;}

/*****************************  分页宽度兼容  **********************************/
#kkpager{width: 100% !important;}
/*****************************  选项  **********************************/
.options.one .flex{display: inline-block;width: 25%;vertical-align: top;}
.options.two .flex{display: inline-block;width: 50%;vertical-align: top;}
/*****************************  空白页  **********************************/
.h_nodata{
    height: 600px;
    line-height: 500px;
}

/*****************************  适应  **********************************/
/*屏幕小于1366*/
@media screen and (max-width:1365px){
    .timing{font-size:12px;}
    .homework_Box,.exercise_box,.publish_box,.exercise_btn_in{width: 900px;}.edit{width: 1000px;}
    .homework_Box dl{font-size: 14px;}
    .h_Type_li_normal a{left: 35%;bottom:6%;font-size: 12px;}
    .h_Type_li_normal a span{font-size: 14px;}
    .h_Type_li_big span a{font-size: 14px;line-height: 2;}
    .h_Minework_table{font-size: 14px;}
    .exercise_btn input,.exercise_btn a,.edit_btn input,.edit_btn a{width: 100px;font-size: 14px;line-height:34px;border-radius: 6px;}
    .exercise_btn_edit{text-indent: 20px;background: #faa249 url("../../image/homework/h_work_btn_01small.png") no-repeat 25px 8px;}
    .exercise_box > h1{font-size: 18px;}
    .exercise_box .timeing{font-size: 12px;}
    .work_group > h2{font-size: 16px;}
    .data_Popup .in{width: 420px;height: 260px;margin-top: -130px;margin-left: -210px;}
    .data_Popup .in .btn a{font-size: 14px; line-height: 32px;}
    .exercise_statistics > dl dt,.exercise_statistics > dl dd{font-size: 12px;}
    .exercise_statistics .statistics dl dt,.exercise_statistics .statistics dl dd{font-size: 12px;}
    .exercise_statistics{width: 240px;}
    .exercise_statistics > dl{line-height: 32px;}
    .exercise_statistics_close{top: -28px;width: 20px;height: 28px;background: url("../../image/homework/exercise_statistics_close_s.png");}
    .exercise_btn.bottom .exercise_btn_in{margin-top: -34px;}
    #audioplayer{top: -44px;}
}
